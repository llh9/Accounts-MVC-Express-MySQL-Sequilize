<section class="container">
  {{!-- <h3 class="gallery-name">{{gallery.name}}</h3> --}}
  {{!-- <h4>Opening Hour: {{format_time gallery.starting_date}}</h4>
  <h4>Closing Hour: {{format_time gallery.ending_date}}</h4> --}}
  {{!-- {{#if gallery.users}}
  <h4>Profiles: {{gallery.users}}</h4>
  {{/if}} 
  {{#if gallery.servicez}}
  <h4>Services: {{gallery.servicez}}</h4>
  {{/if}}
  {{#if gallery.gamez}}
  <h4>Games: {{gallery.gamez}}</h4>
  {{/if}}
  {{#if gallery.videoz}}
  <h4>Videos: {{gallery.videoz}}</h4>
  {{/if}}--}}
  {{#if loggedIn}} 
  <section class="flex-row justify-center">
    {{#each gallery.profiles as |profile|}}
    {{> profile-details}}
    {{/each}}
    {{#each gallery.services as |service|}}
    {{> service-details}}
    {{/each}}
    {{#each gallery.games as |games|}}
    {{> game-details}}
    {{/each}}
    {{#each gallery.videos as |video|}}
    {{> video-details}}
    {{/each}}
    {{#each gallery.paintings as |painting|}}
    {{> painting-details}}
    {{/each}}
  </section>
</section>
{{else}}
<a class="notloggedin" href="/login">Login Required</a>
{{/if}}